{
    "manifest_version": 3,
    "name": "Fake ChatGPT Plus",
    "version": "0.4.2",
    "description": "Make it look like you have ChatGPT Plus",
    "host_permissions": ["https://chat.openai.com/*"],
    "content_scripts": [{
        "matches": ["https://chat.openai.com/*"],
        "js": ["index.js"],
        "run_at": "document_idle"
    }]
}